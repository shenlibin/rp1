<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.test.mapping.userMapper">

	<!-- resultType=User -->
	<select id="getUser" parameterType="int" resultType="com.test.domain.User">
		select *from
		user where id &gt; #{id}
    </select>

	<insert id="addUser" parameterType="com.test.domain.User"
		keyProperty="id">
		insert into
		user(name,id,age)values(#{name},#{id},#{age})
	</insert>

	<insert id="addUsers" parameterType="java.util.List"
		keyProperty="id">
		insert into user(name,age,id)values
		<foreach collection="list" item="var" index="index" separator=",">
			(#{var.name},#{var.age},#{var.id})
		</foreach>


	</insert>
</mapper>